<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ontob.animationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".frame.FrameAnimationActivity"></activity>
        <activity android:name=".view.ViewAnimationActivity"></activity>
        <activity android:name=".view.animation.RotateAnimationActivity">
        </activity>
        <activity android:name=".view.animation.ScaleAnimationActivity"></activity>
        <activity android:name=".view.animation.SetAnimationActivity"></activity>
        <activity android:name=".view.animation.TranslateAnimationActivity"></activity>
        <activity android:name=".view.animation.AlphaAnimationActivity"></activity>
        <activity android:name=".property.PropertyTestActivity"></activity>
        <activity android:name=".property.propertyanimation.ObjectAnimatorActivity"></activity>
        <activity android:name=".property.propertyanimation.ValueAnimatorDemoActivity"></activity>
        <activity android:name=".property.propertyanimation.ViewPropertyAnimatorActivity"></activity>
    </application>

</manifest>