        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".usertestwork.UserTestWorkIndexActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".usertestwork.activity.baseglide.BaseGlideActivity"></activity>
        <activity android:name=".usertestwork.activity.generateapiuutils.GeneratedAPIGlideActivity"></activity>
        <activity android:name=".usertestwork.activity.optionsutils.OpstionClassGlideActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>