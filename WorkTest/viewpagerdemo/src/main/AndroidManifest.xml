<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onetob.diancandemo.viewpagerdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".module1.ViewPagerDemo1Activity"></activity>
        <activity android:name=".module2.ViewPagerDemo2Activity"></activity>
        <activity android:name=".usermolder1.UserViewPagerDemo1Activity"></activity>
        <activity android:name=".usermolder2.UserViewPagerDemo2Activity"></activity>
    </application>

</manifest>